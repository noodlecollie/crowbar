NIConsole--ConsoleWidget
{
	background-color:			#000000;
	font-family:				"Lucida console";
	qproperty-maxLines:			300;
	qproperty-messageColor:		#dde6ff;
	qproperty-warningColor:		#ff1131;
	
	/*1- inactive text*/
	qproperty-customColor1:		#3c3c3c;
	
	/*2 - highlighted text*/
	qproperty-customColor2:		#efdb00;
	
	/*3-6 - arbitrary*/
	/*qproperty-customColor3:		#000000;*/
	/*qproperty-customColor4:		#000000;*/
	/*qproperty-customColor5:		#000000;*/
	/*qproperty-customColor6:		#000000;*/
}

NIConsole--CommandEntryBox
{
	qproperty-iconConCommand:		"resource/command_symbol.svg";
	qproperty-iconConVar:			"resource/variable_symbol.svg";
}

